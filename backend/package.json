{
  "name": "cardstrategy-backend",
  "version": "1.0.0",
  "description": "CardStrategy Backend API with Performance Optimization",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server-enhanced-v2.js",
    "dev": "nodemon src/server.js",
    "dev:simple": "nodemon test-server-simple.js",
    "start:simple": "node test-server-simple.js",
    "dev:basic": "nodemon src/server-simple.js",
    "start:basic": "node src/server-simple.js",
    "dev:minimal": "nodemon src/server-minimal.js",
    "start:minimal": "node src/server-minimal.js",
    "build": "npm run clean && npm run compile",
    "clean": "npm run migrate:undo && npm run seed:undo",
    "compile": "babel src -d dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:performance": "node scripts/performance-test.js",
    "test:security": "npm audit && snyk test",
    "test:all": "jest --testPathPattern=tests",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "migrate": "sequelize-cli db:migrate",
    "migrate:undo": "sequelize-cli db:migrate:undo",
    "seed": "sequelize-cli db:seed:all",
    "seed:undo": "sequelize-cli db:seed:undo:all",
    "setup": "npm run migrate && npm run seed",
    "performance:test": "node scripts/performance-test.js",
    "deep-learning:test": "node scripts/deep-learning-test.js",
    "benchmark": "node scripts/benchmark.js",
    "monitor": "node scripts/monitor.js",
    "health-check": "node scripts/health-check.js",
    "cache:clear": "node scripts/cache-clear.js",
    "db:optimize": "node scripts/db-optimize.js",
    "websocket:test": "node scripts/websocket-test.js",
    "notification:test": "node scripts/notification-test.js",
    "export:test": "node scripts/export-test.js",
    "batch:test": "node scripts/batch-test.js",
    "deploy:production": "npm run build && npm run test && npm run test:security",
    "start:enhanced": "node src/server-enhanced-v2.js",
    "dev:enhanced": "nodemon src/server-enhanced-v2.js",
    "start:test": "node src/server-test-simple.js",
    "dev:test": "nodemon src/server-test-simple.js",
    "start:progressive": "node src/server-enhanced-progressive.js",
    "dev:progressive": "nodemon src/server-enhanced-progressive.js"
  },
  "keywords": [
    "cardstrategy",
    "backend",
    "api",
    "performance",
    "optimization",
    "deep-learning",
    "tensorflow",
    "prediction",
    "cards",
    "investment",
    "caching",
    "monitoring",
    "websocket",
    "real-time",
    "notifications",
    "batch-operations",
    "data-export"
  ],
  "author": "CardStrategy Team",
  "license": "MIT",
  "dependencies": {
    "@tensorflow/tfjs-layers": "^4.10.0",
    "@tensorflow/tfjs-node": "^4.10.0",
    "backoff": "^2.5.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "bull": "^4.12.0",
    "circuit-breaker-js": "^0.0.1",
    "cloudinary": "^2.7.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "cron": "^3.1.6",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.3.1",
    "eventemitter2": "^6.4.9",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "express-ws": "^5.0.2",
    "helmet": "^7.1.0",
    "hpp": "^0.2.3",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "mathjs": "^11.11.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "pdfkit": "^0.14.0",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "redis": "^5.8.2",
    "retry": "^0.13.1",
    "sequelize": "^6.35.0",
    "sharp": "^0.32.6",
    "socket.io": "^4.7.4",
    "technicalindicators": "^3.1.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xss-clean": "^0.1.4",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.4",
    "@babel/core": "^7.23.5",
    "@babel/node": "^7.23.3",
    "@babel/preset-env": "^7.23.5",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "snyk": "^1.1234.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
