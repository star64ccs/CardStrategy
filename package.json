    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "husky": "8.0.3",
    "jest": "29.7.0",
    "jest-expo": "53.0.0",
    "lint-staged": "15.2.0",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "keywords": [
