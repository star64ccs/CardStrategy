{
  "name": "cardstrategy",
  "version": "1.0.0",
  "description": "卡策（CardStrategy）是一款結合 AI 與市場分析的卡牌投資與收藏管理平台",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:optimized": "expo start --optimize",
    "start:clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "setup": "node scripts/setup-environment.js",
    "setup:quick": "bash scripts/quick-start.sh",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/quick-start.ps1",
    "dev:backend": "cd backend && npm run dev",
    "dev:full": "concurrently \"npm run dev:backend\" \"npm run start\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "db:migrate": "cd backend && npm run migrate",
    "db:seed": "cd backend && npm run seed",
    "db:reset": "cd backend && npm run migrate:undo && npm run migrate && npm run seed",
    "db:setup": "node scripts/setup-postgresql.js",
    "db:setup:windows": "node scripts/setup-postgresql-windows.js",
    "check": "node scripts/check-environment.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:e2e": "jest --testPathPattern=e2e",
    "test:data-quality": "jest --testPathPattern=\"dataQuality\" --coverage",
    "detox:build": "detox build",
    "detox:test": "detox test",
    "detox:test:ios": "detox test --configuration ios.sim.debug",
    "detox:test:android": "detox test --configuration android.emu.debug",
    "detox:build:ios": "detox build --configuration ios.sim.debug",
    "detox:build:android": "detox build --configuration android.emu.debug",
    "test:utils": "jest --testPathPattern=utils",
    "test:services": "jest --testPathPattern=services",
    "test:components": "jest --testPathPattern=components",
    "test:screens": "jest --testPathPattern=screens",
    "test:store": "jest --testPathPattern=store",
    "check:services": "node scripts/check-services-status.js",
    "check:postgres": "node scripts/setup-postgresql-production.js",
    "check:redis": "node scripts/setup-redis-production.js",
    "setup:cloudflare": "node scripts/setup-cloudflare.js",
    "cloudflare:quick": "node scripts/setup-cloudflare-quick.js",
    "cloudflare:diagnostic": "node scripts/cloudflare-diagnostic.js",
    "render:staging": "node scripts/setup-render-staging.js",
    "check:auto-deploy": "node scripts/check-auto-deploy-status.js",
    "setup:ssh": "node scripts/setup-ssh-key.js",
    "deploy:render": "curl -X POST \"https://api.render.com/v1/services/$RENDER_SERVICE_ID/deploys\" -H \"Authorization: Bearer $RENDER_TOKEN\" -H \"Content-Type: application/json\"",
    "deploy:digitalocean": "./scripts/deploy-digitalocean.sh",
    "test:tensorflow": "node scripts/test-tensorflow.js",
    "test:enhanced-tensorflow": "node scripts/test-enhanced-tensorflow.js",
    "test:anomaly-detection": "node scripts/test-anomaly-detection.js",
    "test:server": "node scripts/test/test-enhanced-server-simple.js",
    "start:test": "cd backend && npm run start:test",
    "start:progressive": "cd backend && npm run start:progressive",
    "dev:progressive": "cd backend && npm run dev:progressive",
    "test:portfolio-optimization": "node scripts/test-portfolio-optimization.js",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:debug": "jest --detectOpenHandles --forceExit",
    "test:update": "jest --updateSnapshot",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "expo build",
    "build:analyze": "expo build --analyze",
    "build:web": "expo build:web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "eject": "expo eject",
    "preinstall": "npx only-allow npm",
    "prepare": "husky install",
    "ci:setup": "npm ci && npm run lint && npm run type-check",
    "ci:test": "npm run test:ci",
    "ci:build": "npm run build",
    "ci:deploy": "npm run build && echo 'Deployment completed'",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "performance:test": "npm test -- --testPathPattern=\"performance\"",
    "data-quality:test": "npm test -- --testPathPattern=\"dataQuality\" --coverage --verbose",
    "data-quality:report": "npm run data-quality:test && echo 'Data quality test report generated'",
    "api:validate": "npm test -- --testPathPattern=\"dataQuality\" --testNamePattern=\"API.*endpoint\"",
    "coverage:report": "npm run test:coverage && open coverage/lcov-report/index.html",
    "docs:generate": "npm run test:coverage && echo 'Documentation generated'",
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "reset": "npm run clean && npm run ci:setup"
  },
