{
  "name": "cardstrategy",
  "version": "1.0.0",
  "description": "卡策（CardStrategy）是一款結合 AI 與市場分析的卡牌投資與收藏管理平台",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:optimized": "expo start --optimize",
    "start:clear": "expo start --clear",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint:check": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "clean:project": "node scripts/cleanup-project.js",
    "clean:cache": "expo r -c",
    "clean": "rm -rf node_modules && npm install",
    "audit": "npm audit",
    "monitor:security": "npm audit --audit-level moderate",
    "monitor:health": "expo doctor",
    "monitor:performance": "expo start --performance",
    "report:coverage": "npm run test:coverage && open coverage/lcov-report/index.html",
    "report:performance": "expo start --performance --report",
    "report:security": "npm audit --json > security-report.json",
    "deploy:staging": "expo build --platform all --release-channel staging",
    "deploy:production": "expo build --platform all --release-channel production",
    "deploy:vercel": "vercel --prod",
    "deploy:heroku": "git push heroku main",
    "deploy:docker": "docker build -t cardstrategy . && docker run -p 3000:3000 cardstrategy",
    "deploy:frontend": "expo build --platform web",
    "deploy:backend": "cd backend && npm run deploy",
    "deploy:mobile": "expo build --platform all"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "2.0.1",
    "axios": "1.6.2",
    "expo": "53.0.0",
    "expo-camera": "14.0.3",
    "expo-constants": "15.4.5",
    "expo-device": "5.9.3",
    "expo-file-system": "16.0.5",
    "expo-font": "11.10.2",
    "expo-image-picker": "14.7.1",
    "expo-localization": "^16.1.6",
    "expo-location": "16.5.2",
    "expo-notifications": "0.27.6",
    "expo-secure-store": "12.8.1",
    "expo-splash-screen": "0.26.4",
    "expo-status-bar": "1.11.1",
    "i18next": "23.7.11",
    "react": "^19.0.0",
    "react-i18next": "14.0.1",
    "react-native": "0.79.5",
    "react-native-biometrics": "3.0.1",
